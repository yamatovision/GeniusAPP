{
  "name": "appgenius-cli",
  "version": "0.1.0",
  "description": "Command line interface for AppGenius AI-powered development",
  "main": "dist/index.js",
  "bin": {
    "appgenius": "dist/index.js",
    "appgenius-cli": "dist/index.js",
    "appgenius-tool": "dist/tool-cli.js"
  },
  "scripts": {
    "build": "tsc && npm run copy-js",
    "copy-js": "mkdir -p dist/commands && cp src/commands/tool-cli.js dist/commands/ && cp src/services/claude-tool-wrapper.js dist/services/ && cp src/services/claude-tool-wrapper-enhanced.js dist/services/ && cp src/tool-cli.js dist/tool-cli.js && chmod +x dist/tool-cli.js",
    "watch": "tsc -w",
    "start": "node dist/index.js",
    "tool": "node dist/tool-cli.js",
    "tool:offline": "node dist/tool-cli.js --offline",
    "dev": "ts-node src/index.ts",
    "lint": "eslint src --ext .ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:ai": "node test/ai_search_test.js",
    "test:wrapper": "node test/wrapper_test.js",
    "test:md": "node test/wrapper_test.js 'マークダウンファイルを探して詳細を教えてください' --verbose",
    "prepare": "npm run build"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "keywords": [
    "ai",
    "development",
    "cli",
    "code-generator",
    "appgenius"
  ],
  "author": "AppGenius Team",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "axios": "^1.8.1",
    "chalk": "^4.1.2",
    "commander": "^13.1.0",
    "dotenv": "^16.4.7",
    "fs-extra": "^11.3.0",
    "glob": "^10.4.5",
    "inquirer": "^8.2.4",
    "marked": "^4.3.0",
    "marked-terminal": "^4.2.0",
    "node-fetch": "^2.7.0",
    "ora": "^5.4.1",
    "strip-ansi": "^6.0.1",
    "terminal-link": "^2.1.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^8.2.5",
    "@types/node": "^22.13.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
